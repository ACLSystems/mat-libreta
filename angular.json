            "index": "apps/conalepjalisco/src/index.html",
            "main": "apps/conalepjalisco/src/main.ts",
            "polyfills": "apps/conalepjalisco/src/polyfills.ts",
            "tsConfig": "apps/conalepjalisco/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/conalepjalisco/src/favicon.ico",
              "apps/conalepjalisco/src/assets"
            ],
            "styles": ["apps/conalepjalisco/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
