          "options": {
            "jestConfig": "apps/conalepjalisco/jest.config.js",
            "tsConfig": "apps/conalepjalisco/tsconfig.spec.json",
            "setupFile": "apps/conalepjalisco/src/test-setup.ts"
          }
        }
      }
    },
    "conalepjalisco-e2e": {
      "root": "apps/conalepjalisco-e2e",
      "sourceRoot": "apps/conalepjalisco-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/conalepjalisco-e2e/cypress.json",
            "tsConfig": "apps/conalepjalisco-e2e/tsconfig.e2e.json",
            "devServerTarget": "conalepjalisco:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "conalepjalisco:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/conalepjalisco-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/conalepjalisco-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "cetec"
}
